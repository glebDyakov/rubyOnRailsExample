<h1>new article</h1>
<%= form_for :article do |f| %>
    <p>
        # Title<br/>
        <% f.label :email %><br/> 
        <%= f.text_field :email %>
    </p>
    <p>
        # Title<br/>
        <% f.label :message %><br/> 
        <%= f.text_area :message %>
    </p>
    <p>
        # Title<br/>
         
        <%= f.submit %>
    </p>
    
<% end %>